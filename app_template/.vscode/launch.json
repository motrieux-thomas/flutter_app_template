    "version": "0.2.0",
    "configurations": [
        {
            "name": "App",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define=APP_ENV=dev",
                "--dart-define=APP_SUFFIX=.dev",
                "--cache-sksl"
            ]
        },
        {
            "name": "App (profile)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=APP_ENV=dev",
                "--dart-define=APP_SUFFIX=.dev",
                "--cache-sksl"
            ]
        }
    ]
}